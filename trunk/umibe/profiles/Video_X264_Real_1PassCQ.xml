<?xml version="1.0" encoding="ISO-8859-1"?>
<Profile>
	<Name>RealCQ24</Name>
	<EncoderName>x264.exe</EncoderName>
	<!-- 1Pass/2Pass -->
	<Mode>CRF</Mode>
	<!-- each setting as used in command line -->
	<PassOptions1>
		<PassOption1>--crf 24.0</PassOption1>
		<PassOption1>--ref 5</PassOption1>
		<PassOption1>--mixed-refs</PassOption1>
		<PassOption1>--bframes 3</PassOption1>
		<PassOption1>--b-adapt 2</PassOption1>
		<PassOption1>--b-pyramid</PassOption1>
		<PassOption1>--weightb</PassOption1>
		<PassOption1>--deblock -2:-1</PassOption1>
		<PassOption1>--subme 8</PassOption1>
		<PassOption1>--trellis 2</PassOption1>
		<PassOption1>--psy-rd 1.15:0.7</PassOption1>
		<PassOption1>--partitions all</PassOption1>
		<PassOption1>--8x8dct</PassOption1>
		<PassOption1>--me umh</PassOption1>
		<PassOption1>--threads auto</PassOption1>
		<PassOption1>--thread-input</PassOption1>
		<PassOption1>--aud</PassOption1>
		<PassOption1>--progress</PassOption1>
		<PassOption1>--no-psnr</PassOption1>
		<PassOption1>--no-ssim</PassOption1>
	</PassOptions1>	
</Profile>