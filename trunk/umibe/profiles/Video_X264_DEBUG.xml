<?xml version="1.0" encoding="ISO-8859-1"?>
<Profile>
	<Name>AnimeHDCQ</Name>
	<EncoderName>X264</EncoderName>
	<EncoderRoute>./resources/x264.exe</EncoderRoute>
	<!-- 1Pass/2Pass -->
	<EncoderPassNumber>1</EncoderPassNumber>
	<!-- each setting as used in command line -->
	<PassOptions1>
		<PassOption1>--crf 25</PassOption1>
		<PassOption1>--ref 1</PassOption1>
		<PassOption1>--deblock -2:-1</PassOption1>
		<PassOption1>--subme 2</PassOption1>
		<PassOption1>--partitions none</PassOption1>
		<PassOption1>--me dia</PassOption1>
		<PassOption1>--threads auto</PassOption1>
		<PassOption1>--thread-input</PassOption1>
		<PassOption1>--progress</PassOption1>
		<PassOption1>--no-psnr</PassOption1>
		<PassOption1>--no-ssim</PassOption1>
		<PassOption1>--output REPLACEOUTPUT</PassOption1>
		<PassOption1>REPLACEINPUT</PassOption1>
	</PassOptions1>	
</Profile>